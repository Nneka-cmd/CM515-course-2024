Bonous Content
#Data: Exploring historic, current and projected population data across
#countries on infant mortality and birth rate from 1950 to 2100
#url: https://ourworldindata.org/population-growth

#Import population data in csv format
infant_mortality <- read.csv("Infant_mortality.csv")
birth_rate <- read.csv("population-and-demography.csv")

#Explore infant_mortality and change class of variables
str(infant_mortality)
infant_mortality$Country.name <- as.factor(infant_mortality$Country.name)
#infant_mortality$Year <- as.factor(infant_mortality$Year)

#Explore birth_rate and change class of variables
str(birth_rate)
birth_rate$Country.name <- as.factor(birth_rate$Country.name)
#birth_rate$Year <- as.factor(birth_rate$Year)

#load package for manipulation and visualization of data
library(tidyverse)

#explore both files and merge into one data frame
intersect(colnames(infant_mortality), colnames(birth_rate))
population_data <- merge(infant_mortality, birth_rate,
                         by = intersect(colnames(infant_mortality), colnames(birth_rate)))
population_data <- gather(population_data, key = "Category",
            value = "Rate", Infant.mortality.rate, Birth.rate)
str(population_data)
population_data$Category <- as.factor(population_data$Category)

#Isolate a subset of the data by income
income_levels <- population_data %>%
  subset(Country.name == c("High-income countries","Lower-middle-income countries"
                           , "Low-income countries"))

#Visualize the data and save plot as a pdf
pdf(file = "my_plot.pdf", width = 10, height = 4)
ggplot(income_levels, aes(x = Year, y = Rate, color = Category)) + 
  geom_point()+
  facet_wrap(~Country.name, scales = "free")+
  scale_x_continuous(n.breaks = 20)+
  theme(legend.title = element_blank(),
        legend.text = element_text(size = 10, face = "bold"),
        axis.text = element_text(size = 10, face = "bold"),
        axis.title = element_text(size = 10, face = "bold"),
        axis.text.x = element_text(angle = 45, vjust = 0.5, hjust = 0.5),
        strip.text = element_text(size = 10, face = "bold"))
dev.off()